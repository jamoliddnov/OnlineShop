<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link rel="stylesheet" href="fonts/stylesheet.css">
	<link rel="stylesheet" href="~/css/style.css">
	<meta name="description" content="" />
	<meta name="author" content="" />
	<link rel="icon" type="image/x-icon" href="assets/favicon.ico" />
	<link href="~/css/site.css" rel="stylesheet" type="text/css" />
	<link href="~/css/style.css" rel="stylesheet" type="text/css" />

</head>
<body>
	<div class="wrapper">
		<header>
			<div class="header_top">
				<div class="container">
					<nav>
					</nav>
					<div class="registration" style="margin-right:-11%">
						<a asp-controller="accounts" asp-action="login">Kirish</a> / <a asp-controller="accounts" asp-action="register">Ro'yhatdan o'tish</a>
					</div>
				</div>
			</div>
		</header>

		<div class="header_middle" style="background-color:#FCD106; height:200px; margin-top:-40px">
			<div class="container">
				<div class="header_logo" style="margin-left:-10%; height:70%">
					<a href="#">
						<img src="~/images/logo.png" alt="logo" style="margin-top:80px">
					</a>
				</div>

			</div>
		</div>
	</div>

		<div>
			@RenderBody()
		</div>



	<div class="wrapper" style="margin-top: 800px; height:100px">
		<div class="container">
			<section class="products">
				<div class="products_block">
				</div>
			</section>
		</div>

		<main style="margin-bottom:-80px; height:150px">
			<section class="advantages">
				<div class="container">
				</div>
			</section>
		</main>


		<div class="header_bottom">
			<div class="container">
				<nav class="main_nav">
				</nav>
			</div>
		</div>
		<footer style="height:200px;">

		</footer>
		</div>
	<script src="~/lib/jquery/dist/jquery.min.js"></script>
	<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
	<script src="~/js/site.js" asp-append-version="true"></script>
	@await RenderSectionAsync("Scripts", required: false)
</body>
</html>